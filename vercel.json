{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/*.cjs", "use": "@vercel/node" }
  ],
  "functions": {
    "api/*.js": { "runtime": "nodejs18.x" }
  },
  "routes": [
    { "src": "/api/amap-place-text", "dest": "/api/amap-place-text.cjs" },
    { "src": "/api/amap-place-around", "dest": "/api/amap-place-around.cjs" },
    { "src": "/api/amap-input-tips", "dest": "/api/amap-input-tips.cjs" },
    { "src": "/api/(.*)", "dest": "/api/$1.cjs" }
  ]
}
